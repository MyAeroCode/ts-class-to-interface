{
  "name": "ts-class-to-interface",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.ts",
  "scripts": {
    "start": "ts-node src/main.ts",
    "test": "jest"
  },
  "jest": {
    "verbose": true,
    "testTimeout": 120000,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@core/(.*)": "<rootDir>/src/core/$1",
      "@core": "<rootDir>/src/core",
      "@test/(.*)": "<rootDir>/src/test/$1",
      "@test": "<rootDir>/src/test"
    },
    "setupFilesAfterEnv": [
      "./setupJest.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MyAeroCode/ts-class-to-interface.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MyAeroCode/ts-class-to-interface/issues"
  },
  "homepage": "https://github.com/MyAeroCode/ts-class-to-interface#readme",
  "devDependencies": {
    "@types/jest": "^29.1.1",
    "@types/node": "^18.8.1",
    "jest": "^29.1.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
